{
  "name": "comment",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "tag"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_text"
            },
            "named": false,
            "value": "text"
          }
        ]
      }
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_user"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "_user": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^()]+"
          },
          "named": true,
          "value": "user"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_text": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_stop_char"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n\\s/'\"<\\(\\[\\{\\.,:;!\\?\\\\\\}\\]\\)>-]+"
        }
      ]
    },
    "_stop_char": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "-"
        }
      ]
    },
    "__newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "__newline"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "name"
    },
    {
      "type": "SYMBOL",
      "name": "invalid_token"
    }
  ],
  "inline": [],
  "supertypes": []
}

